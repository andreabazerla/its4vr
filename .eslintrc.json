{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
      "no-underscore-dangle": 1,
      "no-unused-vars": 1,
      "no-console": 1,
      "no-undef": 1,
      "max-len": 1,
      "no-trailing-spaces": 1,
      "padded-blocks": 1,
      "prefer-const": 1
    }
}
